					Height=1.2
					Text='年 初 数'
				End
				Item
					Type=StaticBox
					Name='amt_end1'
					BorderStyles=[DrawLeft|DrawTop]
					TextAlign='middleCenter'
					Left=6.58
					Top=3.8000002
					Width=2.42
					Height=1.2
					Text='期 末 数'
				End
				Item
					Type=StaticBox
					Name='asset'
					BorderStyles=[DrawLeft|DrawTop]
					TextAlign='middleCenter'
					Left=9.0
					Top=3.8000002
					Width=3.5200005
					Height=1.2
					Text='负债和所有者权益\r\n（或股东权益）'
				End
				Item
					Type=StaticBox
					Name='value2'
					BorderStyles=[DrawLeft|DrawTop]
					WordWrap=T
					TextAlign='middleCenter'
					Left=12.52
					Top=3.8000002
					Width=0.6499996
					Height=1.2
					Text='行次'
				End
				Item
					Type=StaticBox
					Name='amt_begin2'
					BorderStyles=[DrawLeft|DrawTop]
					TextAlign='middleCenter'
					Left=13.17
					Top=3.8000002
					Width=2.42
					Height=1.2
					Text='年 初 数'
				End
				Item
					Type=StaticBox
					Name='amt_end2'
					BorderStyles=[DrawLeft|DrawTop|Drawright]
					TextAlign='middleCenter'
					Left=15.59
					Top=3.8000002
					Width=2.42
					Height=1.2
					Text='期 末 数'
				End
			End
		End
		Item
			Type=ReportFooter
			Name='reportFooter1'
			RepeatOnPage=T
			PrintAtBottom=T
			Height=2.0
			Items Control
				Item
					Type=StaticBox
					Name='charge'
					Left=0.0
					Top=0.0
					Width=4.2
					Height=1.5
					Text='企业负责人：'
				End
				Item
					Type=StaticBox
					Name='accounting'
					Left=4.2
					Top=0.0
					Width=4.0
					Height=1.5
					Text='主管会计：'
				End
				Item
					Type=StaticBox
					Name='tab'
					Left=8.2
					Top=0.0
					Width=3.4000006
					Height=1.5
					Text='制表：'
				End
				Item
					Type=StaticBox
					Name='reportDate'
					Left=11.6
					Top=0.0
					Width=3.3000002
					Height=1.5
					Text='报出日期：'
				End
				Item
					Type=StaticBox
					Name='date'
					Left=14.900001
					Top=0.0
					Width=3.0999994
					Height=1.5
					Text='年     月     日'
				End
			End
		End
	End
End
